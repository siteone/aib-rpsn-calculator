{
  "name": "aib-rpsn-calculator",
  "version": "1.0.2",
  "description": "Airbank RPSN calculator (local-specific Czech calculator)",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "lint": "gulp lint",
    "compile": "NODE_ENV=production babel src --out-dir lib && echo 'Successfully compiled'",
    "prepublish": "npm test && npm run lint && npm run compile",
    "ci:upload-coverage": "cat ./coverage/lcov.info | coveralls",
    "ci": "npm run lint && npm run compile && npm test -- --coverage && npm run ci:upload-coverage"
  },
  "author": "Adam Bisek <adam.bisek@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.42.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-flowtype": "^1.0.0",
    "jest": "^19.0.2"
  },
  "peerDependencies": {
    "moment": "^2.0.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!*.js",
      "!**/lib/**",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  }
}
